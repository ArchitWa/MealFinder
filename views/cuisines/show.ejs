<h2><%= cuisine.type %></h2>
<a href="/cuisines/<%= cuisine.id %>/edit">Edit</a>
<%- include('../partials/deleteForm', { url: `/cuisines/${cuisine.id}` }) %> 
<% if (recipesInCuisine.length > 0) { %>
    <h2>Recipes For Cuisine</h2>
    <% recipesInCuisine.forEach(recipe => { %>
        <img height="150" width="150" alt="avatar" style="border-radius: 50%;" src="<%= recipe.foodImagePath %> ">
    <% }) %>
<% } %>