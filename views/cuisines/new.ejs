<h2
    class="text-2xl font-extrabold pl-3 pt-5 mb-2 text-center text-transparent bg-clip-text bg-gradient-to-r from-white to-slate-600">
    New Cuisine</h2>
<form action="/cuisines" method="POST">
    <%- include("_form_fields") %>
    <div class="flex items-center justify-center pt-2 pl-2 my-2 ">
        <a href="/cuisines" class="text-red-700 h-9 pt-[6px] hover:text-white border-2 border-red-600 hover:bg-red-700 focus:ring-4 focus:outline-none focus:ring-red-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-2 mb-2 "><i class="fa-solid fa-square-xmark"></i> Cancel</a>
        <button type="submit" class="text-green-700 h-9 pt-[6px] hover:text-white border-2 border-emerald-600 hover:bg-emerald-700 focus:ring-4 focus:outline-none focus:ring-emerald-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-2 mb-2">Add <i class="fa-solid fa-square-plus"></i></button>
    </div>
</form>